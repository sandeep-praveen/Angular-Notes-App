        "html": "<label class=\"form-label\">Password:</label>\n<div class=\"d-flex align-items-center justify-content-end\">\n    <input type=\"{{ hidePassword ? 'password' : 'text' }}\" class=\"form-control\" formControlName=\"pwd\"\n                (keyup.enter)=\"Authenticate()\" autocomplete=\"off\">\n    <i *ngIf=\"authForm.get('pwd')?.value != ''\" [ngClass]=\"hidePassword ? 'bi bi-eye-slash' : 'bi bi-eye'\" class=\"position-absolute pe-3\" (click)=\"togglePassword()\"></i>\n</div>",
        "css": "",
        "js": "togglePassword() {\n    this.hidePassword = !this.hidePassword;\n}",
